class Main {
    public static void main(String[] args) {
        int [][]arr=new int[3][3];
        
        arr[0][0]=1;
         arr[0][1]=2;
          arr[0][2]=3;
           arr[1][0]=4;
         arr[1][1]=5;
          arr[1][2]=6;
           arr[2][0]=7;
         arr[2][1]=8;
          arr[2][2]=9;
        
   
            System.out.print(arr[2][0]+" ");
        }


   public class Main {
    public static void main(String[] args) {
        int x = 3;
        int y = 3;
        int[][] arr = new int[x][y];
        int num = 1;

        for (int i = 0; i < x; i++) {
            for (int j = 0; j < y; j++) {
              
                if (num == 2) {
                    arr[i][j] = 8;
                } else if (num == 8) {
                    arr[i][j] = 2;
                } else {
                    arr[i][j] = num;
                }
                num++;
            }
        }

       
        for (int i = 0; i < arr.length; i++) {
            for (int j = 0; j < arr[i].length; j++) {
                System.out.print(arr[i][j] + "\t");
            }
            System.out.println();
        }
    }
}


output:
   1	8	3	
4	5	6	
7	2	9	

=== Code Execution Successful ===

public class Main {
    public static void main(String[] args) {
        int x = 3;
        int y = 3;
        int[][] arr = new int[x][y];
        int num = 2;

        for (int i = 0; i < x; i++) {
            for (int j = 0; j < y; j++) {
                    arr[i][j]=num;
                   num+=2;
                
                 
            }
        }

       
        for (int i = 0; i < arr.length; i++) {
            for (int j = 0; j < arr[i].length; j++) {
                System.out.print(arr[i][j] + "\t");
              
            }
            System.out.println();
        }
    }
}
